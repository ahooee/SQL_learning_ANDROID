<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ir.linuxian.sql">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sql_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sql_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activityha.EntekhabActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>

        <activity android:name=".activityha.AmoozeshFehrestActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>

        <activity android:name=".activityha.AghazActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activityha.AmoozeshActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>